#include<bits/stdc++.h>
using namespace std;
#define int long long
int32_t main(){
  int q;
  cin>>q;
  int t=0;
  while(q--){
      int n,m;
      cin>>n>>m;
      int arr[n];
      int sum=0;
      for(int i=0;i<n;i++){
          cin>>arr[i];
          sum+=arr[i];
      }
      cout<<"Case #"<<++t<<": "<<(sum%m)<<endl;
  }
  return 0;
}
